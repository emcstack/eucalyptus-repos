[eucalyptus]
name=Eucalyptus 4.2 - $basearch
#mirrorlist=http://mirrors.eucalyptus.com/mirrors?product=eucalyptus&distro=rhel&releasever=$releasever&basearch=$basearch&version=4.2
baseurl={{ eucalyptus_repo_url }}
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-eucalyptus-release
gpgcheck=1
enabled=1

[eucalyptus-source]
name=Eucalyptus 4.2 - Source
mirrorlist=http://mirrors.eucalyptus.com/mirrors?product=eucalyptus&distro=rhel&releasever=$releasever&basearch=source&version=4.2
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-eucalyptus-release
gpgcheck=1
enabled=0
